Heart[] hearts = new Heart[30];
Heart h1;

public void settings() {
  size(600,600);
}

void setup() {  
  h1 = new Heart(); 
  for(int u = 0; u <20; u ++) {
    hearts[u] = new Heart();
  }
}

void draw() {
  h1.display(); 
  
}

void mousePressed() {
 for(int u = 0; u <20; u ++) {
    hearts[u].ascend();
  }   
}